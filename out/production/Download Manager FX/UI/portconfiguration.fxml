<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>


<AnchorPane prefHeight="100.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Portconfiguration">
   <children>
      <BorderPane prefHeight="100.0" prefWidth="400.0">
         <top>
            <TabPane prefHeight="150.0" prefWidth="400.0" tabClosingPolicy="UNAVAILABLE" BorderPane.alignment="CENTER">
              <tabs>
                <Tab text="Tracker">
                  <content>
                    <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                           <children>
                              <Label layoutX="14.0" layoutY="26.0" prefHeight="30.0" prefWidth="50.0" text="Port" />
                              <TextField fx:id="trackerportfield" layoutX="65.0" layoutY="29.0" promptText="Enter Port" />
                              <Button fx:id="trackerportsave" layoutX="237.0" layoutY="29.0" mnemonicParsing="false" text="Save" />
                           </children>
                        </AnchorPane>
                  </content>
                </Tab>
                <Tab text="Server">
                  <content>
                    <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                           <children>
                              <Label layoutX="14.0" layoutY="26.0" prefHeight="30.0" prefWidth="50.0" text="Port" />
                              <TextField fx:id="serverportfield" layoutX="65.0" layoutY="29.0" promptText="Enter Port" />
                              <Button fx:id="serverportsave" layoutX="237.0" layoutY="29.0" mnemonicParsing="false" text="Save" />
                           </children>
                        </AnchorPane>
                  </content>
                </Tab>
              </tabs>
            </TabPane>
         </top>
      </BorderPane>
   </children>
</AnchorPane>
